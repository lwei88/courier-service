{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "func-names": 0,
    "no-underscore-dangle": ["error", { "allow": ["_id"] }]
  }
}
